ELEMENT ZR HCP_A3 91.224 5566.3 39.181 !

FUNCTION VV0000 1 0.0; 10000 N !

TYPE_DEFINITION % SEQ * !
DEFINE_SYSTEM_DEFAULT ELEMENT 2 !

TYPE_DEFINITION ^ GES AMEND_PHASE_DESCRIPTION BCC_A2 MAGNETIC -1.0 0.4 !
TYPE_DEFINITION & GES AMEND_PHASE_DESCRIPTION FCC_A1 MAGNETIC -3.0 0.28 !
PHASE BCC_A2 %^  1 1.0 !
CONSTITUENT BCC_A2 :CR, FE, HF, MO, NB, TA, TI, V, W, ZR: !

PHASE FCC_A1 %&  1 1.0 !
CONSTITUENT FCC_A1 :AL, CR, FE, HF, MO, NB, TA, TI, V, W, ZR: !

PHASE HCP_A3 %  1 1 !
CONSTITUENT HCP_A3 :CR, FE, HF, MO, NB, TA, TI, V, W, ZR: !

PARAMETER V0(FCC_A1,AL;0) 1 10.1092E-6; 10000 N LU2005 !
PARAMETER VA(FCC_A1,AL;0) 1 4.87439E-5*T + 2.66296E-8/2*T**2 + -1*-0.076014*T**(-1); 10000 N LU2005 !


